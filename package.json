{
  "name": "zip-iterator",
  "version": "1.1.7",
  "description": "Extract contents from zip archive type using an iterator API using streams or paths. Use stream interface and pipe transforms to add decompression algorithms",
  "keywords": [
    "extract",
    "iterator",
    "asyncIterator",
    "Symbol.asyncIterator",
    "extract-zip",
    "decompress",
    "decompress-zip",
    ".zip",
    "zip",
    "yauzl",
    "zip-stream",
    "stream"
  ],
  "homepage": "https://github.com/kmalakoff/zip-iterator",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/zip-iterator.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "buffer-v6-polyfill": "^1.0.5",
    "call-once-fn": "^1.0.1",
    "end-of-stream": "^1.4.4",
    "extract-base-iterator": "^1.1.5",
    "fs-access-compat": "^1.0.3",
    "lifecycle": "^1.0.4",
    "lodash.compact": "^3.0.1",
    "mkdirp-classic": "^0.5.3",
    "os-shim": "^0.1.3",
    "pinkie-promise": "^2.0.1",
    "queue-cb": "^1.4.4",
    "readable-stream": "^2.3.7",
    "rimraf2": "^2.8.2",
    "short-hash": "^1.0.0",
    "temp-suffix": "^0.1.1",
    "zip": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "fs-iterator": "^5.1.1",
    "fs-stats-spys": "^1.1.0",
    "ts-dev-stack": "^1.5.3",
    "unbzip2-stream": "^1.4.3"
  },
  "packageManager": "npm@11.0.0+sha512.11dff29565d2297c74e7c594a9762581bde969f0aa5cbe6f5b3644bf008a16c065ece61094d9ffbb81125be38df8e1ba43eb8244b3d30c61eb797e9a2440e3ec",
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs"
  }
}
