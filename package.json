{
  "name": "zip-iterator",
  "version": "0.3.2",
  "description": "Extract contents from zip archive type using an iterator API using streams or paths. Use stream interface and pipe transforms to add decompression algorithms",
  "keywords": [
    "extract",
    "iterator",
    "asyncIterator",
    "Symbol.asyncIterator",
    "extract-zip",
    "decompress",
    "decompress-zip",
    ".zip",
    "zip",
    "yauzl",
    "zip-stream",
    "stream"
  ],
  "homepage": "https://github.com/kmalakoff/zip-iterator",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/zip-iterator.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply-unsafe src/ test/",
    "test": "tsds test:node --timeout=10000",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "buffer-v6-polyfill": "^1.0.5",
    "end-of-stream": "^1.4.4",
    "extract-base-iterator": "^1.0.0",
    "fs-access-compat": "^1.0.3",
    "lifecycle": "^1.0.4",
    "lodash.compact": "^3.0.1",
    "mkpath": "^1.0.0",
    "once": "^1.4.0",
    "os-shim": "^0.1.3",
    "queue-cb": "^1.1.7",
    "readable-stream": "^2.3.7",
    "rimraf": "^2.7.1",
    "short-hash": "^1.0.0",
    "temp-suffix": "^0.1.1",
    "zip": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "fs-iterator": "^4.2.0",
    "fs-stats-spys": "^1.0.2",
    "just-extend": "^6.2.0",
    "ts-dev-stack": "^0.14.0",
    "unbzip2-stream": "^1.4.3"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs",
      "esm"
    ]
  }
}
